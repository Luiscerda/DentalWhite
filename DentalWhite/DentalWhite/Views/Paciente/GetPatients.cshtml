<script src="~/Content/plugins/jquery/jquery.min.js"></script>
<script src="~/Scripts/App/Paciente/GetPacientes.js"></script>
@{
    ViewBag.Title = "Pacientes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <h3 class="card-title">Listado</h3>
            </div>
            <div class="card-body">
                <table id="tablePacientes" class="table table-bordered table-striped">
                    <thead>
                        <tr>
                            <th>Tipo ID</th>
                            <th>Identificacion</th>
                            <th>Nombre</th>
                            <th>Apellido</th>
                            <th>Edad</th>
                            <th>Celular</th>
                            <th></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

